class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        minimum =  sys.maxint
        profit = 0
        
        for i in range(0, len(prices)):
            if prices[i] < minimum:
                minimum = prices[i]
            elif prices[i] - minimum > profit:
                profit = prices[i] - minimum
        return profit
