from collections import deque 
class Solution(object):
    def firstUniqChar(self, s):
        hash_map = {}
        
        for letter in s:
            if letter in hash_map.keys():
                hash_map[letter] += 1
            else:
                hash_map[letter] = 1
        for i in range(len(s)):
            if hash_map[s[i]] == 1:
                return i
        return -1
              
