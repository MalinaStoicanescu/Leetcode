class Solution(object):
    def isAnagram(self, s, t):
        
        hash_map = {}
        
        for elem in s:
            if elem in hash_map.keys():
                hash_map[elem] += 1
            else:
                hash_map[elem] = 1
        
        for elem in t:
            if elem in hash_map.keys():
                hash_map[elem] -= 1
            else:
                return False
        
        for key in hash_map.keys():
            if hash_map[key] != 0:
                return False
        return True
        
