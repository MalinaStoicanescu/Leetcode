class Solution(object):
    def rotate(self, nums, k):
        """
        :type nums: List[int]
        :type k: int
        :rtype: None Do not return anything, modify nums in-place instead.
        """
        hash_map = {}
        i = 0
        for element in nums:
            hash_map[i] = element
            i += 1
        
        for elem in hash_map.keys():
            index = elem + k
            if index >= len(nums):
                index = index%len(nums)
            nums[index] = hash_map[elem]
        
        return nums
